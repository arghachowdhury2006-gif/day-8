#include <stdio.h>
int main()
{
    
    int arr[5]={1,2,3,4,5};
    int i,max=arr[0],min=arr[0];  
    for (i = 0; i <= 4; i++)
    {
        printf("%d ", arr[i]);
    }
    for (i = 0; i <= 4; i++)
    {
        if(max<arr[i])
        max=arr[i];
         if(min>arr[i])
        min=arr[i];
    }
    printf("\nThe maximum number is %d",max);
 printf("\nThe minimum number is %d",min);
    return 0;
}
