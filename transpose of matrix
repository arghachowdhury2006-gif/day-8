#include<stdio.h>
int main() {
    int a[3][3]={{1,2,3},{4,5,6},{7,8,9}}, trans[3][3];
    int i,j;
    for(i=0;i<=2;i++){
        for(j=0;j<=2;j++){
           printf("%d", a [i][j]);
        }
        printf("\n");
    }
    for(i=0;i<=2;i++){
        for(j=0;j<=2;j++){
          trans[j][i]=a [i][j];
        }
        printf("\n");
    }
    for(i=0;i<=2;i++){
        for(j=0;j<=2;j++){
          printf("%d", trans[i][j]);
        }
        printf("\n");
    }
    
    
    return 0;
}
