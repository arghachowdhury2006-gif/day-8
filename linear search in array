#include <stdio.h>
#include <stdbool.h>
int main()
{
    int arr[5] = {1, 2, 3, 4, 5};
    int i, x = 4;
    bool flag = false;
    for (i = 0; i <= 4; i++)
    {
        if (arr[i] == x)
        {
            flag = true;
            break;
        }
    }
    if (flag == false)
    {
        printf("%d is not present in array:", x);
    }
    else
    {
        printf("%d is present at the index 0f %d:", x, i);
    }

    return 0;
}
