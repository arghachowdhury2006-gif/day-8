#include <stdio.h>

int main() {
   int arr[6]={1,2,3,4,5,6};

    int even_count = 0,odd_count = 0;
    for (int i = 0; i < 5; i++) {
        if (arr[i] % 2 == 0) {
            even_count++;
        } else { 
            odd_count++;
        }
    }

    printf("\nOccurrence of Even numbers: %d\n", even_count);
    printf("Occurrence of Odd numbers: %d\n", odd_count);

    return 0;
}
